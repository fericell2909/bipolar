{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.0",
        "artesaos/seotools": "^0.12.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "cviebrock/eloquent-sluggable": "^4.3",
        "doctrine/dbal": "^2.6",
        "dyrynda/laravel-model-uuid": "4.1.1",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "~4.0",
        "genealabs/laravel-caffeine": "^0.8",
        "guzzlehttp/guzzle": "^6.3",
        "ip2location/ip2location-laravel": "^1.0",
        "jaybizzle/laravel-crawler-detect": "^1.2",
        "jenssegers/agent": "^2.6",
        "jenssegers/date": "^3.4",
        "kitetail/zttp": "^0.3.0",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.2",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/telescope": "^2",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.5.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "~2.1.0",
        "mcamara/laravel-localization": "^1.3",
        "orangehill/iseed": "^2.5",
        "predis/predis": "^1.1",
        "ramsey/uuid-doctrine": "^1.6",
        "rap2hpoutre/laravel-log-viewer": "^1.0",
        "rebing/graphql-laravel": "^4.0",
        "shipping-docker/vessel": "^5.0",
        "spatie/laravel-activitylog": "^3.2",
        "spatie/laravel-analytics": "^3.6",
        "spatie/laravel-model-status": "^1.0",
        "spatie/laravel-newsletter": "^4.1",
        "spatie/laravel-sitemap": "^5.2",
        "spatie/laravel-translatable": "^3.1",
        "spatie/regex": "^1.3",
        "vinkla/hashids": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0",
        "nunomaduro/collision": "^2.0",
        "filp/whoops": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-pcntl": "7.2",
            "ext-posix": "7.2",
            "ext-exif": "7.2"
        }
    }
}
